<script>
  var something = document.body;
  var body = document.body, html = document.documentElement;
  var height = Math.max( body.scrollHeight, body.offsetHeight, html.clientHeight, html.scrollHeight, html.offsetHeight, window.innerHeight );
  var pattern = Trianglify({
      width: window.innerWidth,
      height: height,
      cell_size: Math.random()*200 + 40,
      x_colors: 'random',
      variance: Math.random()
  });
  something.style['background-image'] = 'url(' + pattern.png() + ')';
</script>
